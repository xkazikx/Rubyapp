<h1>Posty</h1>
<%= link_to 'New post', new_post_path %>
<hr>
<% @posts.each do |post|%>
<h2><%= link_to post.title, post_path(id: post.id)%></h2>
<p><%= post.content %></p>
<p><%= post.created_at %></p>
<p><%= post.updated_at %></p>
<p>Author: <<%= post.user.email %></p>
<%= link_to 'Edit post', edit_post_path(id: post.id) %>
<%= link_to 'Delete post', post_path(id: post.id), method: :delete %>
<hr>
<%end%>
